if game:GetService("RunService"):IsClient() then
	if game.PlaceId==93528621380632 then _G.text("Client does not support","error","Script") else error("Client does not support") end script:Destroy()
end
script.Parent=owner.PlayerGui
local tween=game:GetService("TweenService")
local rad=math.rad
local cf=CFrame.new
local clamp=math.clamp
local v3=Vector3.new
local sine=0
local sin=math.sin
local cos=math.cos
local ray=Ray.new
local attack=false
local angles=function(x,y,z)
	return CFrame.Angles(rad(x),rad(y),rad(z))
end
local char=script.Parent.Parent.Character
local t=char.Torso
local hum=char.Humanoid
local speed=.5
local rs=Instance.new("RemoteEvent",char)
rs.Name="ГђВ§ГђВўГђЕѕГђЕѕГђЕѕ"
NLS([==[local player=game:GetService("Players").LocalPlayer
local char=player.Character
local hum=char:FindFirstChildOfClass("Humanoid")
local rut=char.HumanoidRootPart
local h=char.Head
local RS=game:GetService("RunService").RenderStepped
local rs=char:WaitForChild("ГђВ§ГђВўГђЕѕГђЕѕГђЕѕ")
local mouse=player:GetMouse()
mouse.Button1Up:Connect(function()
	rs:FireServer()
end)
while true do hum.CameraOffset=hum.CameraOffset:Lerp((rut.CFrame*CFrame.new(0,1.5,0)):PointToObjectSpace(h.Position),.1)RS:Wait()end]==])
local welds={}
Part0 = Instance.new("Part")
SpecialMesh1 = Instance.new("SpecialMesh")
Sound2 = Instance.new("Sound")
Part0.Size = Vector3.new(1, 1, 1)
Part0.BottomSurface = Enum.SurfaceType.Smooth
Part0.CanCollide = false
Part0.TopSurface = Enum.SurfaceType.Smooth
SpecialMesh1.Name = "SpecialMesh"
SpecialMesh1.Parent = Part0
SpecialMesh1.MeshId = "rbxassetid://17472464885"
SpecialMesh1.TextureId = "http://www.roblox.com/asset/?id=17472488021"
SpecialMesh1.MeshType = Enum.MeshType.FileMesh
Sound2.Parent = Part0
Sound2.SoundId = "rbxassetid://4571259077"
Sound2.Volume = 2
local create=function(a,d,e,f,g)
	local weld=Instance.new("Weld",g or t)
	weld.Part0=d
	weld.Part1=a
	weld.C0=e
	if f then weld.C1=f end
	return weld
end
local Raycast=function(og,dic,black,max,water)
	return workspace:FindPartOnRayWithIgnoreList(ray(og,dic.Unit*max),black or {},false,water)
end
local bucket=Part0
bucket.Parent=char
local rut=char.HumanoidRootPart
table.insert(welds,create(bucket,char["Right Arm"],cf(-1.69999981,0.028778553,-1.7995863,0,1,0,0.474869698,0,0.880056322,0.880056322,0,-0.474869698)))
table.insert(welds,create(char["Right Arm"],t,cf(1,0.5,0)*angles(0,90,0),cf(-0.5,0.5,0)*angles(0,90,0)))
table.insert(welds,create(char["Left Arm"],t,cf(-1,0.5,0)*angles(0,-90,0),cf(0.5,0.5,0)*angles(0,-90,0)))
table.insert(welds,create(char["Right Leg"],t,cf(1,-1,0)*angles(0,90,0),cf(0.5,1,0)*angles(0,90,0)))
table.insert(welds,create(char["Left Leg"],t,cf(-1,-1,0)*angles(0,-90,0),cf(-0.5,1,0)*angles(0,-90,0)))
table.insert(welds,create(t,rut,cf(0,0,0)*angles(-90,-180,0),cf(0,0,0)*angles(-90,-180,0)))
table.insert(welds,create(char.Head,t,cf(0,1,0)*angles(-90,-180,0),cf(0,-0.5,0)*angles(-90,-180,0)))
la0=welds[3].C0
rutjno0=welds[6].C0
ll0=welds[5].C0
rl0=welds[4].C0
ra0=welds[2].C0
n0=welds[7].C0
h0=welds[1].C0
rs.OnServerEvent:Connect(function(plr)
	if plr.Character~=char or attack then return end
	Sound2:Play()
	attack=true
	tween:Create(welds[1],TweenInfo.new(0.3,Enum.EasingStyle.Back,Enum.EasingDirection.InOut),{C0=cf(-0.775536954,-1.25518799,0.237118214,-0.819825232,0.571628392,-0.0335779414,0.286930263,0.359351486,-0.887996614,-0.495537698,-0.737636328,-0.458622843)}):Play()
	tween:Create(welds[3],TweenInfo.new(0.3,Enum.EasingStyle.Back,Enum.EasingDirection.InOut),{C0=cf(-1.274845,0.26373291,-0.563724279,0.940678179,-0.323869675,0.10115841,-0.049569428,-0.426116347,-0.903309286,0.335659742,0.844708979,-0.416892409)}):Play()
	tween:Create(welds[2],TweenInfo.new(0.3,Enum.EasingStyle.Back,Enum.EasingDirection.InOut),{C0=cf(1.11585391,0.315565944,-0.497962952,0.895096958,0.445765704,0.00971095264,0.153410614,-0.287452221,-0.945429444,-0.418648273,0.847741067,-0.325683057)}):Play()
	task.wait(0.3)
	tween:Create(welds[1],TweenInfo.new(0.3,Enum.EasingStyle.Back,Enum.EasingDirection.Out),{C0=cf(-1.0618633,-1.60218084,0.822972417,-0.303627253,0.384651095,0.871695936,-0.837006092,0.329435587,-0.436913311,-0.455226928,-0.862273633,0.221929625)}):Play()
	tween:Create(welds[3],TweenInfo.new(0.3,Enum.EasingStyle.Back,Enum.EasingDirection.Out),{C0=cf(-1.29573393,-0.167058706,-0.56880188,0.940678179,-0.282091916,-0.188543707,-0.049569428,0.435466588,-0.898839056,0.335659742,0.854864299,0.395650804)}):Play()
	tween:Create(welds[2],TweenInfo.new(0.3,Enum.EasingStyle.Back,Enum.EasingDirection.Out),{C0=cf(1.20256507,-0.105818987,-0.466982365,0.895096958,0.272344291,0.353029788,0.153410584,0.555317342,-0.817367077,-0.418648243,0.785781384,0.455282032)}):Play()
	task.wait(0.3)
	attack=false
end)
bucket.Touched:Connect(function(hit)
	if attack and hit.Parent:FindFirstChild("Humanoid") and not hit.Parent:FindFirstChild("ГђВ§ГђЕѕГђЕѕ") then
