local forw,side=0,0
local function lerp(a,b,c)
	return a+((b-a)*c)
end
game:GetService("RunService").Heartbeat:Connect(function(dt)
	sine = sine + dt * 60
	local Direction=hum.MoveDirection
	local WSVal=clamp((Direction*t.CFrame.RightVector).X+(Direction*t.CFrame.RightVector).Z,-1,1)
	local Stand=Raycast(t.CFrame*cf(t.Size.X/4,-t.Size.Y/2,0).Position,-rut.CFrame.UpVector,{char},4,false)
	local Moving=(t.Velocity*v3(1,0,1)).Magnitude>.01
	forw,side=lerp(forw,clamp((Direction*rut.CFrame.LookVector).X+(Direction*rut.CFrame.LookVector).Z,-1,1),.15),lerp(side,WSVal,.15)
	if not Moving and Stand then
		speed=.2
		if not attack then
			welds[1].C0=welds[1].C0:Lerp(h0*cf(0,1.192,-1.846)*angles(-16.658,-59.23+sin(sine/40)*1,-28.255),speed)
			welds[2].C0=welds[2].C0:Lerp(ra0*cf(-0.135,-0.4+sin(sine/55)*0.03,-0.1)*angles(-9.725,24.749,82.623+sin(sine/40)*1),speed)
			welds[3].C0=welds[3].C0:Lerp(la0*cf(0,-0.474+sin(sine/55)*0.03,-0)*angles(-3,-19.612,-87.876,-sin(sine/40)*1),speed)
		end
		welds[6].C0=welds[6].C0:Lerp(rutjno0*cf(0,0,0)*angles(sin(sine/25)*0.2,0,0),speed)
		welds[5].C0=welds[5].C0:Lerp(ll0*cf(0,0,0)*angles(0,7.836,0),speed)
		welds[7].C0=welds[7].C0:Lerp(n0*cf(0,0,0)*angles(2.797+sin(sine/25)*1,0,0),speed)
		welds[4].C0=welds[4].C0:Lerp(rl0*cf(0,0,0)*angles(0,-8.114,0),speed)
	elseif Moving and Stand then
		speed=.3
		if not attack then
			welds[1].C0=welds[1].C0:Lerp(h0*cf(0,1.192,-1.846)*angles(-16.658,-59.23,-28.255),speed)
			welds[3].C0=welds[3].C0:Lerp(la0*cf(0,-0.474+sin(sine/4)*0.06,-0)*angles(-3,-19.612,-74.35),speed)
			welds[2].C0=welds[2].C0:Lerp(ra0*cf(-0.135,-0.4+sin(sine/4)*0.06,-0.1)*angles(-9.725,24.749,69),speed)
		end
		welds[6].C0=welds[6].C0:Lerp(rutjno0*cf(0,0,sin(sine/4)*0.1)*angles(forw*10,0,side*10),.2/(t.Size.Y/2)*(hum.WalkSpeed/16)/1)
		welds[5].C0=welds[5].C0:Lerp(ll0*cf(-.1*WSVal,cos(sine/4)*0.1,0)*angles(-8*WSVal,0,sin(sine/5)*35),speed)
		welds[7].C0=welds[7].C0:Lerp(n0*cf(0,0,-0)*angles(2.748+sin(sine/6)*2,-8*WSVal,-8*WSVal),speed)
		welds[4].C0=welds[4].C0:Lerp(rl0*cf(.1*WSVal,cos(sine/4)*0.1,0)*angles(8*WSVal,0,sin(sine/5)*35),speed)
	else
		speed=.1
		if not attack then
			welds[3].C0=welds[3].C0:Lerp(la0*cf(0,-0.474,-0)*angles(-3,-19.612,-122.746),speed)
			welds[2].C0=welds[2].C0:Lerp(ra0*cf(-0.135,-0.4,-0.1)*angles(-9.725,24.749,117.494),speed)
			welds[1].C0=welds[1].C0:Lerp(h0*cf(0.781,1.143,-2.33)*angles(30,-57.649,13.1),speed)
		end
		welds[6].C0=welds[6].C0:Lerp(rutjno0*cf(0,0,0)*angles(-11.485,0,0),speed)
		welds[5].C0=welds[5].C0:Lerp(ll0*cf(-0.759,0.144,0)*angles(0,0,0),speed)
		welds[7].C0=welds[7].C0:Lerp(n0*cf(0,0,0)*angles(2.748,-8*WSVal,-8*WSVal),speed)
		welds[4].C0=welds[4].C0:Lerp(rl0*cf(0,0,0)*angles(0,0,-11.495),speed)
	end
end)
